source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'
xcodeproj 'CameraApp.xcodeproj'

# Analytics
pod 'Heap', '2.1.3'

# A drop-in universal solution
# for moving text fields
# out of the way of the keyboard in iOS.
pod 'TPKeyboardAvoiding', '1.2.6'

# AWS Services
pod 'AWSiOSSDKv2', '2.1.1'
pod 'AWSCognitoSync', '2.1.1'

# Stripe SDK and related libraries
pod 'Stripe', '3.1.0'
pod 'Stripe/ApplePay', '3.1.0'
pod 'PaymentKit', '1.1.1'
pod 'ApplePayStubs', '0.1.4'
